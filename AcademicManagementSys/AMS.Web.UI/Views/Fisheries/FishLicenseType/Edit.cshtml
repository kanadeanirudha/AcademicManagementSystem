@model AMS.ViewModel.FishLicenceTypeMasterViewModel

<form id="FormEditFishLicenseType" method="post">
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js") " type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <div id="update-message" class="error invisible"></div>
   
    @Html.HiddenFor(model => model.ID)

    <div class="widget-block">
        <div class="widget-head">
            <h5>Edit Fish License Type</h5>
        </div>

        <div class="widget-content">
            <div class="widget-box" style="padding: 15px">
                <div class="control-group control-label">
                    <span style="color: red; float: left; padding-left: 1px; font-weight: bold">*</span>
                    @Html.LabelFor(model => model.LicenseType, new { style = "font-Weight:Bold;" })
                    <div class="controls input-xlarge text-tip">
                        @Html.TextBoxFor(model => model.LicenseType, new { style = "font-Weight:Bold;", maxlength = "20" })
                        <br />
                        @Html.ValidationMessageFor(model => model.LicenseType)
                    </div>
                </div>
                <div class="form-actions" style="border: 1px solid #e5e5e5; text-align: right">
                    <input type="button" id="EditFishLicenseTypeMasterRecord" value="@Resources.ButtonAction_Submit" class="btn btn-primary" />
                </div>
            </div>
        </div>
    </div>
</form>


<script type="text/javascript">
    $(document).ready(function () {
        FishLicenseType.Initialize();
    });
</script>