
@model AMS.ViewModel.FishItemLicenseRateViewModel

<style>
    .ui-front {
        /*z-index:10000;*/
        z-index: 9999;
    }
</style>

<form id="FormCreateFishItemLicenseRate" method="post">
    <link href="@Url.Content("~/Content/jquery-ui.theme.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("Content/admin/jquery-ui-1.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js") " type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

    @Html.HiddenFor(model => model.CentreCode)
    @Html.HiddenFor(model => model.LicenseTypeID)
    @Html.HiddenFor(model => model.ItemID)
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <div id="update-message" class="error invisible"></div>

    <div class="widget-block">
        <div class="widget-head">
            <h5>Create Fish Item License Rate </h5>
        </div>
        <div class="widget-content">
            <div class="widget-box" style="padding: 15px">
                <div class="control-group control-label">
                    <span style="color: red; float: left; padding-left: 1px; font-weight: bold">*</span>
                    @Html.LabelFor(model => model.ItemName, new { style = "font-Weight:Bold;" })
                    <div class="controls input-xlarge text-tip">
                        @Html.TextBoxFor(model => model.ItemName, new { id = "ItemName", maxlength = "50", style = "font-weight: bold;", @placeholder = "Search Item" })
                        <br />
                        @Html.ValidationMessageFor(model => model.ItemName)
                    </div>
                </div>

                <div class="control-group control-label">
                    <span style="color: red; float: left; padding-left: 1px; font-weight: bold">*</span>
                    @Html.LabelFor(model => model.Rate, new { style = "font-Weight:Bold;" })
                    <div class="controls input-xlarge text-tip">
                        @Html.TextBoxFor(model => model.Rate, new { style = "font-Weight:Bold;", maxlength = "20", Value = "" })
                        <br />
                        @Html.ValidationMessageFor(model => model.Rate)
                    </div>
                </div>

                <div class="form-actions" style="border: 1px solid #e5e5e5; text-align: right">
                    <input type="button" id="CreateFishItemLicenseRateRecord" value="@Resources.ButtonAction_Submit" class="btn btn-primary" />
                    <input name="" id="reset" type="submit" class="btn btn-secondary" value="@Resources.ButtonAction_Reset" />
                </div>
            </div>
        </div>
    </div>
</form>

<script type="text/javascript">
    $(document).ready(function () {
        FishItemLicenseRate.Initialize();
    });
</script>