@model AMS.Web.UI.Models.SemesterInfoMaster.SemesterInfoMaster
@{
    ViewBag.Title = "Details";
}

@Styles.Render("~/Content/Vendors")
@Html.Partial("_LeftMenu")

<div id='dlgEdit'>
    <div class="col-md-10">
        <div class="row">
            <div class="panel panel-default bootstrap-admin-no-table-panel">
                <div class="panel-heading">
                    <div class="text-muted bootstrap-admin-box-title">Semester Master</div>
                </div>
                <div class="bootstrap-admin-no-table-panel-content bootstrap-admin-panel-content collapse in">
                    <form class="form-horizontal">
                        <fieldset>
                              <legend>Details</legend>
                                                            
                                  <div class="form-group">
                            <label class="col-lg-2 control-label" for="disabledInput">Semester Name</label>
                            <div class="col-lg-2">
                                 <div class="display-field"><span class="form-control">@Model.SemesterName</span></div>
                            </div>
                        </div>
                            

                             <div class="form-group">
                            <label class="col-lg-2 control-label" for="disabledInput">Semester Code</label>
                            <div class="col-lg-2">
                               <div class="display-field"><span class="form-control">@Model.SemesterCode</span></div>
                            </div>
                        </div>

                               <div class="form-group">
                            <label class="col-lg-2 control-label" for="disabledInput">Semester Type</label>
                            <div class="col-lg-2">
                               <div class="display-field"><span class="form-control">@Model.SemesterType</span></div>
                            </div>
                        </div>

                            
                                 <br />
                            </fieldset>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<div>
    @Html.ActionLink("Back to List", "Index")
</div>
