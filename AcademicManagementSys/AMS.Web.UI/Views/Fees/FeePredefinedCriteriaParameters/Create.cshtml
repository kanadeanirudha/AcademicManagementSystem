@model AMS.ViewModel.FeePredefinedCriteriaParametersViewModel
<div id="custom-content">
    <div class="block-header">
        <h2>&nbsp;<i class="zmdi zmdi-view-toc"></i>&nbsp;Create Fee Predefined Criteria Parameters</h2>
    </div>
    <div class="card">
        <div class="card-body card-padding">
            <div id="displayErrorMessage" role="alert" hidden class="alert">
                <button class="close" type="button"><span aria-hidden="true">×</span></button>
                <p id="messageContent"></p>
            </div>
            <form id="FormCreateFeePredefinedCriteriaParameters">

                <div class="col-sm-12">
                    <div class="col-sm-6">
                        <span style="color: red; float: left; padding-left: 1px; font-weight: bold">*</span>
                        @Html.LabelFor(model => model.FeeCriteriaParametersDescription, new { })
                        <div class="form-group  fg-line">
                            @Html.TextBoxFor(model => model.FeeCriteriaParametersDescription, new { @class = "form-control", style = "", maxlength = "50", })
                        </div>
                        @Html.ValidationMessageFor(model => model.FeeCriteriaParametersDescription)
                    </div>

                    <div class="col-sm-6">
                        <span style="color: red; float: left; padding-left: 1px; font-weight: bold">*</span>
                        @Html.LabelFor(model => model.FeeCriteriaParametersCode, new { })
                        <div class="form-group fg-line">
                            @Html.TextBoxFor(model => model.FeeCriteriaParametersCode, new { @class = "form-control", style = "", maxlength = "40", })
                        </div>
                        @Html.ValidationMessageFor(model => model.FeeCriteriaParametersCode)
                    </div>
                </div>

                <div class="col-sm-12">
                    <div class="col-sm-6">
                        @Html.LabelFor(model => model.TableEntityName, new { })
                        <div class="form-group fg-line select">
                            @Html.DropDownListFor(model => model.TableEntityName, Model.TableEntityNameListItems, "---Select Table---", new { @class = "form-control", style = "" })
                        </div>
                    </div>

                    <div class="col-sm-6">
                        <span style="color: red; float: left; padding-left: 1px; font-weight: bold">*</span>
                        @Html.LabelFor(model => model.PrimaryKeyFieldName, new { })
                        <div class="form-group fg-line select">
                            @Html.DropDownListFor(model => model.PrimaryKeyFieldName, ViewBag.PrimaryKeyFieldNameList as List<SelectListItem>, "---Select Primary Key ---", new { @class = "form-control", style = "", maxlength = "10" })
                        </div>
                        @Html.ValidationMessageFor(model => model.PrimaryKeyFieldName)
                    </div>
                </div>

                <div class="col-sm-12">
                    <div class="col-sm-6">
                        <span style="color: red; float: left; padding-left: 1px; font-weight: bold">*</span>
                        @Html.LabelFor(model => model.DisplayKeyFieldName, new { })
                        <div class="form-group  fg-line">
                            @Html.DropDownListFor(model => model.DisplayKeyFieldName, ViewBag.DisplayKeyFieldNameList as List<SelectListItem>, "---Select Display Key ---", new { @class = "form-control input-sm", style = "", maxlength = "10" })
                        </div>
                        @Html.ValidationMessageFor(model => model.DisplayKeyFieldName)
                    </div>
                </div>

                <div class="form-group" style="margin-left: 2%; ">
                    <button type="button" id="CreateFeePredefinedCriteriaParametersRecord" class="btn btn-primary">@Resources.ButtonAction_Submit</button>
                </div>
            </form>

        </div>
    </div>
</div>
<script src="@Url.Content("~/Scripts/jquery.validate-1.15.0.js") " type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        FeePredefinedCriteriaParameters.Initialize();

    });
</script>