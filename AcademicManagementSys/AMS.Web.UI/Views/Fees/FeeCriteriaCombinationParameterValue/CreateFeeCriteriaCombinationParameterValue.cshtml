@model AMS.ViewModel.FeeCriteriaCombinationParameterValueViewModel

<form id="FormCreateFeeCriteriaCombinationParameterValue" method="post">
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js") " type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    @Html.HiddenFor(model => model.FeeCriteriaMasterID)
    @Html.HiddenFor(model => model.FeeCriteriaParameterValueCombinationIDs)
    @Html.HiddenFor(model => model.FeeCriteriaValueCombinationDescription)


    <div id="update-message" class="error invisible"></div>

    <div class="widget-block">
        <div class="widget-head">
            <h5>Create Fee Criteria Combination Value</h5>
        </div>
        <div class="widget-content">
            <div class="widget-box" style="padding: 15px">

                <div class="control-group control-label">
                    <div class="controls input-xlarge text-tip">
                        <b>@Resources.DisplayName_Areyousuretoallcate <u>@Html.DisplayFor(model => model.FeeCriteriaValueCombinationDescription)</u> @Resources.DisplayName_FeeCriteriaCombinationValue</b>
                    </div>
                </div>
                <br />

                <div class="form-actions" style="border: 1px solid #e5e5e5; text-align: right">
                    <input type="button" id="CreateFeeCriteriaCombinationParameterValueRecord" value="Yes" class="btn btn-primary" />
                    <input name="" id="closeBtn" type="button" class="btn btn-secondary" value="No" />
                    @*<input type="button" id="CreateFeeCriteriaCombinationParameterValueRecord" value="@Resources.ButtonAction_Submit" class="btn btn-primary" />*@
                    @*   <input name="" id="reset" type="submit" class="btn btn-primary" value="@Resources.ButtonAction_Reset" />*@
                </div>
            </div>
        </div>
    </div>
</form>
<script type="text/javascript">
    $(document).ready(function () {
        debugger;
        FeeCriteriaCombinationParameterValue.Initialize();
    });
</script>
