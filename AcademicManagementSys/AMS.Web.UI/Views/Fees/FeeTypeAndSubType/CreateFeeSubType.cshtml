@model AMS.ViewModel.FeeTypeAndSubTypeViewModel

<div id="custom-content">
    <div class="block-header">
        <h2>&nbsp;<i class="zmdi zmdi-view-toc"></i>&nbsp; &nbsp;@Resources.DisplayName_CreateFeeSubType</h2>
    </div>
    <div class="card">
        <div class="card-body card-padding">
            <div id="displayErrorMessage" role="alert" hidden class="alert">
                <button class="close" type="button"><span aria-hidden="true">×</span></button>
                <p id="messageContent"></p>
            </div>
            <form id="FormCreateFeeSubTypeMaster" method="post" role="form" class="row">
                @Html.HiddenFor(model => model.ID)
                
                <div class="col-sm-12">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.FeeTypeDesc)</label>
                            @Html.TextBoxFor(model => model.FeeTypeDesc, new { @class = "form-control input-sm", disabled = "disabled", style = "background-color:white;" })
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group fg-line select">
                            <label>@Html.LabelFor(model => model.SelectedBalanceSheet)</label>
                            @Html.TextBoxFor(model => model.SelectedBalanceSheet, new { @class = "form-control input-sm", disabled = "disabled", style = "background-color:white;" })
                        </div>

                    </div>
                </div>
                
              

                <div class="col-sm-12">
                    <div class="col-sm-6">
                        <div class="form-group fg-line">
                            <label><span style="color:red">*</span>@Html.LabelFor(model => model.FeeSubTypeDesc)</label>
                           @Html.TextBoxFor(model => model.FeeSubTypeDesc, new { @class = "form-control input-sm", maxlength = "50" })
                        </div>
                        @Html.ValidationMessageFor(m => m.FeeSubTypeDesc)
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group fg-line">
                            <label><span style="color:red">*</span>@Html.LabelFor(model => model.FeeSubShortDesc, new { style = "font-Weight:Bold;" })</label>
                            @Html.TextBoxFor(model => model.FeeSubShortDesc, new { @class = "form-control", maxlength = "10" })
                        </div>
                        @Html.ValidationMessageFor(m => m.FeeSubShortDesc)
                    </div>
                </div>

                <div class="col-sm-12">
                    <div class="col-sm-6">
                        <div class="form-group fg-line select">
                            <label><span style="color:red">*</span>@Html.LabelFor(model => model.AccountID, new { style = "font-Weight:Bold;" })</label>
                            @Html.DropDownListFor(model => model.AccountID, Model.AccountMasterListItems, "---Select  Account---", new { @class = "form-control" })
                        </div>
                       @Html.ValidationMessageFor(m => m.AccountID)
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group fg-line select">
                            <label>@Html.LabelFor(model => model.FeeSource)</label>
                            @Html.DropDownList("FeeSource", ViewData["FeeSource"] as List<SelectListItem>, new { @class = "form-control" })
                        </div>
                       
                    </div>
                </div>

                <div class="col-sm-12">
                    <div class="col-sm-6">
                        <div class="form-group fg-line select">
                            <label>@Html.LabelFor(model => model.CarryForwardFeeSubtypeID)</label>
                            @Html.DropDownListFor(model => model.CarryForwardFeeSubtypeID, Model.FeeSubTypeListItems, Resources.ddlHeaders_SelectCarryFwdSubType, new { @class = "form-control" })
                        </div>
                       @Html.ValidationMessageFor(m => m.CarryForwardFeeSubtypeID)
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group fg-line select">
                            <label>@Html.LabelFor(model => model.SubTypeIdentification)</label>
                           @Html.DropDownList("SubTypeIdentification", ViewData["SubTypeIdentification"] as List<SelectListItem>, new { @class = "form-control" })
                        </div>
                        
                    </div>
                </div>



              <div class="form-group col-sm-12" style="margin-left:1%">
                    <button type="button" id="CreateFeeSubTypeRecord" class="btn btn-primary">@Resources.ButtonAction_Submit</button>&nbsp;
                    <input name="" id="reset" type="submit" class="btn btn-secondary" value="@Resources.ButtonAction_Reset" />
                </div>

            </form>
        </div>
    </div>
</div>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js") " type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

<script type="text/javascript">
    $(document).ready(function () {
        FeeTypeAndSubType.Initialize();
    });
</script>
