            buttons: {
                Create: function () {
                    // perform login
                    $.post("@Url.Action("Login", "StudyCenterMaster")",
				    {

				    },
				    function (data, status, xhr) {
				        if (data.Success) {
				            alert('great'); // do something
				            $('#dlgLogin').dialog("close");
				            $('#divLoginButton').load("@Url.Action("GetLoginButton", "StudyCenterMaster")");
                        } else {
				            // do something else
                        }
				    });
                },
                Cancel: function () {
                    $(this).dialog("close");
                }
            }
        });
    });
</script>

