@model AMS.ViewModel.HMAppointmentTransactionViewModel
<input type="hidden" id="TaskCode" value="HMPatientMedicine" />
@Html.HiddenFor(model => model.TaskCode)
@Html.HiddenFor(model => model.GeneralPatientMasterId)
@Html.HiddenFor(model => model.CentreCode)
@Html.HiddenFor(Model => Model.IsCaseClosed)
<div id="custom-content">
    <div class="card">
        <div class="card-body card-padding">
            <form id="FormCreateHMAppointmentTransaction" role="form" class="row">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="form-group fg-line"><span style="color:red">*</span>
                            @Html.LabelFor(model => model.Symtomp, new { })
                            @Html.TextBoxFor(model => model.Symtomp, new { maxlength = "50", @class = "form-control input-sm ", disabled = "disabled" })
                        </div>
                        @Html.ValidationMessageFor(model => model.Symtomp)
                    </div>
                    <div class="col-sm-12">
                        <div class="form-group fg-line"><span style="color:red">*</span>
                            @Html.LabelFor(model => model.CaseHistoryNumber, new { })
                            @Html.TextBoxFor(model => model.CaseHistoryNumber, new { @class = "form-control input-sm ", disabled = "disabled" })
                        </div>
                        @Html.ValidationMessageFor(model => model.CaseHistoryNumber)
                    </div>
                    <div class="col-sm-3">
                        <div class="form-group fg-line"><span style="color:red">*</span>
                            @Html.LabelFor(model => model.TransactionDate, new { })
                            @Html.TextBoxFor(model => model.TransactionDate, new { @class = "form-control input-sm ", disabled = "disabled" })
                        </div>
                        @Html.ValidationMessageFor(model => model.TransactionDate)
                    </div>
                    <div class="col-sm-3">
                        <div class="form-group fg-line"><span style="color:red">*</span>
                            @Html.LabelFor(model => model.Name, new { })
                            @Html.TextBoxFor(model => model.Name, new { @class = "form-control input-sm ", disabled = "disabled" })
                        </div>
                        @Html.ValidationMessageFor(model => model.Name)
                    </div>
                    <div class="col-sm-3">
                        <div class="form-group fg-line"><span style="color:red">*</span>
                            @Html.LabelFor(model => model.DosesDescription, new { })
                            @Html.TextBoxFor(model => model.DosesDescription, new { maxlength = "50", @class = "form-control input-sm ", disabled = "disabled" })
                        </div>
                        @Html.ValidationMessageFor(model => model.DosesDescription)
                    </div>
                    <div class="col-sm-3">
                        <div class="form-group fg-line"><span style="color:red">*</span>
                            @Html.LabelFor(model => model.ForNumberOfDays, new { })
                            @Html.TextBoxFor(model => model.ForNumberOfDays, new { maxlength = "50", @class = "form-control input-sm ", disabled = "disabled" })
                        </div>
                        @Html.ValidationMessageFor(model => model.ForNumberOfDays)
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
<script src="@Url.Content("~/Scripts/jquery.validate-1.15.0.js") " type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        HMAppointmentTransaction.Initialize();
    });
</script>

