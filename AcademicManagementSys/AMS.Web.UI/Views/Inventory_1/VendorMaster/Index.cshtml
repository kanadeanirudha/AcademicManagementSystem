@model AMS.ViewModel.VendorMasterViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutV2.cshtml";
}

<script src="@Url.Content("~/Scripts/AMS_Scripts/Inventory_1/VendorMaster.js")" type="text/javascript"></script>

<div id="ListViewModel" class="container">
    @Html.HiddenFor(m => m.errorMessage)

     
</div>
<script type="text/javascript">
    $(document).ready(function () {
        //VendorMaster.Initialize();
        if ($('input[name=errorMessage]').val() != "NoMessage") {
            var splitedMsg = ($('input[name=errorMessage]').val()).split(',');
            notify(splitedMsg[0], splitedMsg[1]);
            $('input[name=errorMessage]').val("NoMessage");
        }
        VendorMaster.LoadList();

    });
</script>




