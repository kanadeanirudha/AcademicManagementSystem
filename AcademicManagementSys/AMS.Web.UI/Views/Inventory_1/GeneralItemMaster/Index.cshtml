@model AMS.ViewModel.GeneralItemMasterViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutV2.cshtml";
}
<style type="text/css">
    .mfp-content {
        width: 50%;
    }
</style>
<script src="@Url.Content("~/Scripts/AMS_Scripts/Inventory_1/GeneralItemMaster.js")" type="text/javascript"></script>

<div id="ListViewModel" class="container">
    @Html.HiddenFor(m => m.errorMessage)

</div>
<script type="text/javascript">
    $(document).ready(function () {
        //GeneralItemMaster.Initialize();
        if ($('input[name=errorMessage]').val() != "NoMessage") {
            var splitedMsg = ($('input[name=errorMessage]').val()).split(',');
            notify(splitedMsg[0], splitedMsg[1]);
            $('input[name=errorMessage]').val("NoMessage");
        }
        GeneralItemMaster.LoadList();

    });
</script>




