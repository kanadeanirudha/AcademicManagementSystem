@model AMS.ViewModel.CRMSaleReportViewModel
@Html.HiddenFor(m => m.ReportList)   
<div class="col-sm-6 col-md-3">
    <a class='mfpAjaxModal' href="~/CRMSaleDashboard/GetTodaysMeetingList">
        <div class="mini-charts-item bgm-cyan">
            <div class="clearfix">
                <div class="chart myBarClassMeetings col-sm-6 text-center"></div>
                <div class="count text-center">
                    <small>Today's Meeting</small>
                    <h2>@Html.DisplayFor(m => m.ReportCount)</h2>
                    <small>&nbsp;</small>
                </div>
            </div>
        </div>
    </a>
</div>
<script>
    $(document).ready(function () {
        sparklineBar('myBarClassMeetings', $("#ReportList").val().split(','), '45px', 3, '#fff', 2);
    });
</script>