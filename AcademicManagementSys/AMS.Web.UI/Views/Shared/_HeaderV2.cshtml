                    <a data-toggle="dropdown" href="" id="NotiCountHeaderDiv">
                        <i class="tm-icon zmdi zmdi-notifications"></i>
                        <i class="tmn-counts" id="NotiCountHeader" style="display: none;"></i>
                    </a>
                </li>
                @*-------------------Jump to _RoleList action method of _Header Controller for fetching roles--------------------*@
                <li id="ddlRole" class="dropdown">
                    <a data-toggle="dropdown" href="#"><i class="zmdi tm-icon zmdi-accounts-alt zmdi-hc-fw"></i></a>
                    <ul id="ddlRoles" class="dropdown-menu dm-icon pull-right">
                        @{ Html.RenderAction("_RoleListV2", "_Header", new { PersonID = @HttpContext.Current.Session["PersonId"] }); }

                    </ul>
                </li>

                @*-------------------Jump to _ModuleList action method of _Header Controller for fetching modules--------------------*@

                <li id="ddlModule" class="dropdown">
                    @{ Html.RenderAction("_ModuleListV2", "_Header", new { AdminRoleMasterID = @HttpContext.Current.Session["RoleID"] != null ? @HttpContext.Current.Session["RoleID"] : @HttpContext.Current.Session["DefaultRoleID"] }); }

                </li>

                @*-------------------Jump to _BalancesheetList action method of _Header Controller for fetching Balancesheet List--------------------*@
                @if (HttpContext.Current.Session["UserType"] != null)
                {
                    if (HttpContext.Current.Session["UserType"].ToString() == "E")
                    {
                        <li id="ddlMfdsodule" class="dropdown">
                            @{Html.RenderAction("_BalancesheetListV2", "_Header", new { ModuleName = @HttpContext.Current.Session["ModuleName"] });}
                        </li>
                    }
                }


