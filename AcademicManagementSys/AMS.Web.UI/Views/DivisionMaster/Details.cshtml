@model AMS.Web.UI.Models.DivisionMaster
@{
    ViewBag.Title = "Details";
}

@Styles.Render("~/Content/Vendors")
@Html.Partial("_LeftMenu")

<div id='dlgEdit'>
    <div class="col-md-10">
        <div class="row">
            <div class="panel panel-default bootstrap-admin-no-table-panel">
                <div class="panel-heading">
                    <div class="text-muted bootstrap-admin-box-title">Division Master</div>
                </div>
                <div class="bootstrap-admin-no-table-panel-content bootstrap-admin-panel-content collapse in">
                    <form class="form-horizontal">
                        <fieldset>
                              <legend>Details</legend>
                                
                                <div class="display-label">Division Name</div>
                                <div class="display-field">@Model.DivisionName</div>
                                 <br />
                             <div class="display-label">Division ShortCode</div>
                                <div class="display-field">@Model.DivisionShortCode</div>
                                 <br />
                             <div class="display-label">Division PrintCode</div>
                                <div class="display-field">@Model.DivisionPrintCode</div>
                                 <br />
                            </fieldset>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<div>
    @Html.ActionLink("Back to List", "Index")
</div>
