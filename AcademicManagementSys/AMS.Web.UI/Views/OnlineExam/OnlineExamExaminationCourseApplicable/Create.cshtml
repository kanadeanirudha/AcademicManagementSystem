@model AMS.ViewModel.OnlineExamExaminationCourseApplicableViewModel
<div id="custom-content">
  <div class="block-header">
    <h2>&nbsp;<i class="zmdi zmdi-view-toc"></i>&nbsp; &nbsp;Create Course Details </h2>
      </div>
        <div class="card">
        @Html.HiddenFor(model => model.SemesterMstID)
        @Html.HiddenFor(model => model.ExaminationStatus)
        @Html.HiddenFor(model => model.OnlineExamExaminationMasterId)
        <div class="card-body card-padding">
            <div id="displayErrorMessage" role="alert" hidden class="alert">
                <button class="close" type="button"><span aria-hidden="true">×</span></button>
                <p id="messageContent"></p>
                  </div>
                   <form id="FormCreateOnlineExamExaminationCourseApplicable" role="form" class="row">
                     <div class="form-group col-sm-6">
                       <span style="color:red">*</span>
                         @Html.LabelFor(model => model.CourseYearID, new { })
                           <div class="form-group fg-line select">
                             @Html.DropDownListFor(model => model.CourseYearID, (SelectList)ViewBag.OrganisationCourseYearDetailsListt, "---Select Course Year---", new { @class = "form-control input-sm" })
                        </div>
                         @Html.ValidationMessageFor(model => model.CourseYearID)
                        </div>
                         <div class="form-group col-sm-6">
                           <div class="form-group fg-line select">
                             @Html.LabelFor(model => model.Semester, new { })
                              <span style="color:red">*</span>
                               @Html.DropDownListFor(model => model.Semester, Model.SemesterListItems, "SemesterList", new { @class = "form-control input-sm " })
                                </div>
                              @Html.ValidationMessageFor(model => model.Semester)
                              </div>
                      <div class="form-group col-sm-6">
                     <div class="form-group fg-line ">
                     @Html.LabelFor(model => model.DepartmentID, new { })
                     <span style="color:red">*</span>
                        @Html.DropDownListFor(model => model.DepartmentID, Model.DepartmentListItem, "DepartmentList", new { @class = "form-control input-sm " })

                    </div>
                    @Html.ValidationMessageFor(model => model.DepartmentID)
                </div>
                <div class="form-group col-sm-6">
                <div class="form-group fg-line select">
                    @Html.LabelFor(model => model.ExaminationStatus, new { })
                     <span style="color:red">*</span>
                     @Html.DropDownList("ExaminationStatusID", ViewData["ExaminationStatus"] as List<SelectListItem>, "---Select Examination Status ---", new { @class = "form-control input-sm", data_val = "true", data_toggle = "tooltip", data_placement = "top", data_original_title = "ExaminationStatus*" })

                 </div>
                        @Html.ValidationMessageFor(model => model.ExaminationStatus)
                    </div>
                    <div class="form-group col-sm-6">
                        <div class="form-group fg-line">
                            @Html.LabelFor(model => model.ExaminationStartDate, new { })
                            
                               
                                @Html.TextBoxFor(model => model.ExaminationStartDate, new { @class = "form-control input-sm " })
                            </div>
                            @Html.ValidationMessageFor(model => model.ExaminationStartDate)
                        </div>
                    <div class="form-group col-sm-6">
                    <div class="form-group fg-line">
                        @Html.LabelFor(model => model.ExaminationEndDate, new { })
                       
                     @Html.TextBoxFor(model => model.ExaminationEndDate, new { @class = "form-control input-sm " })
                 </div>
                    @Html.ValidationMessageFor(model => model.ExaminationEndDate)
                </div>

                <div class="form-group col-sm-12">
                     <div class="form-group fg-line">
                       @Html.LabelFor(model => model.ResultAnnounceDate, new { })
                       <span style="color:red">*</span>
                       @Html.TextBoxFor(model => model.ResultAnnounceDate, new { @class = "form-control input-sm " })
                     </div>
                    @Html.ValidationMessageFor(model => model.ResultAnnounceDate)
                </div>

                @*@<div class="form-group col-sm-6">
                        <div class="form-group fg-line">
                            <span style="color:red">*</span> @Html.LabelFor(model => model.MovementCode, new { })
                            @Html.TextBoxFor(model => model.MovementCode, new { maxlength = "20", @class = "form-control input-sm " })

                        </div>
                        @Html.ValidationMessageFor(model => model.MovementCode)
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group fg-line">
                            <label style="margin-top: 2%; margin-bottom: 3%;">
                                @Html.LabelFor(model => model.IsActive, new { style = "padding-left: 0px; margin-top:2%;" })

                            </label>
                        </div>
                        <div class="checkbox col-sm-6">
                            <input type="checkbox" value="" id="IsActive">
                            <i class="input-helper"></i>
                        </div>
                    </div>*@

                <div class="form-group col-sm-12">
                    <input class="btn btn-primary" style="margin-left:1%" value="@Resources.ButtonAction_Submit" id="CreateOnlineExamExaminationCourseApplicableRecord" type="button" />
                </div>

            </form>
        </div>
    </div>
</div>

<script src="@Url.Content("~/Scripts/jquery.validate-1.15.0.js") " type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        OnlineExamExaminationCourseApplicable.Initialize();
    });
</script>

