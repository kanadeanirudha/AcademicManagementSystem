@model AMS.ViewModel.OnlineExamExaminationQuePaperDetailsViewModel
<div id="custom-content">
    <div class="block-header">
        <h2>&nbsp;<i class="zmdi zmdi-view-toc"></i>&nbsp; &nbsp;Add Questions to Paper</h2>
    </div>
    <div class="card">
        <div class="card-body card-padding">
            <div id="displayErrorMessage" role="alert" hidden class="alert">
                <button class="close" type="button"><span aria-hidden="true">×</span></button>
                <p id="messageContent"></p>
            </div>
            <form id="FormCreateOnlineExamSubjectGroupSchedule" role="form" class="row">
                <div class="row">
                    <div class="form-group col-sm-6">
                        <div class="form-group fg-line">
                            Select Question Bank
                            @Html.DropDownListFor(model => model.OnlineExamQuestionBankMasterID, (SelectList)ViewBag.QuestionBankMasterList, "---Select Question Bank ---", new { @class = "form-control input-sm", data_val = "true", data_toggle = "tooltip", data_placement = "top", data_original_title = "" })
                        </div>
                        @Html.ValidationMessageFor(model => model.OnlineExamQuestionBankMasterID)
                    </div>
                    <div class="form-group col-sm-3">
                        <div class="form-group fg-line">
                            <button type="button"  class="btn btn-primary" id="btnShowList">@Resources.GlobalProperties_ShowList</button>
                        </div>
                    </div>
                </div>
                <div class="row">
                    

                </div>
             </form>
        </div>
    </div>
</div>

<script src="@Url.Content("~/Scripts/jquery.validate-1.15.0.js") " type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        OnlineExamExaminationQuePaperDetails.Initialize();
    });
</script>

