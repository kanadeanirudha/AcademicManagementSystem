@model AMS.ViewModel.OnlineExamExaminationQuePaperDetailsViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutV2.cshtml";
}

<script src="@Url.Content("~/Scripts/AMS_Scripts/OnlineExam/OnlineExamExaminationQuePaperDetails.js")" type="text/javascript"></script>

<div class="container">
    <div class="block-header">
        <h2 style="text-transform: none;"><i class="zmdi zmdi-view-toc"></i>&nbsp;View Question for Examination</h2>
    </div>
      <div class="card" style="margin-bottom: 0px;">
        <div class="card-header">
            <ul class="actions">
                <li class="dropdown">
                    <a href="@Url.Action("Index", "OnlineExamExaminationQuePaperDetails")">
                        <i class="zmdi zmdi-long-arrow-return"></i>
                    </a>
                </li>
            </ul>
        </div>
    @Html.HiddenFor(m => m.OnlineExamExaminationQuestionPaperID)
    @Html.HiddenFor(m => m.OnlineExamSubjectGroupScheduleID)
    @Html.HiddenFor(m => m.IsFinal)
    <div id="ListViewModel"></div>
   </div>  
</div>

<script type="text/javascript">
    $(document).ready(function () {
        OnlineExamExaminationQuePaperDetails.LoadViewQuestionList();
    });
</script>
