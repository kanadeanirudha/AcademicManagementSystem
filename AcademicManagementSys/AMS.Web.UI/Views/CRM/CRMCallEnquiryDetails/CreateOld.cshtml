@model AMS.ViewModel.CRMCallEnquiryDetailsViewModel
<form id="FormCreateCRMCallEnquiryDetails" method="post">
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js") " type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <div id="update-message" class="error invisible"></div>

    <div class="widget-block" style="width: auto; height: auto;">
        <div class="widget-head">
            <h5>Create CRM Call Enquiry Details</h5>
        </div>
        <div class="widget-content">
            <div class="widget-box" style="padding: 15px">

                <div class="control-group control-label">


                    <span style="color: red; float: left; padding-left: 1px; font-weight: bold">*</span>
                    @Html.LabelFor(model => model.UploadDate, new { style = "font-Weight:Bold;" })
                    <div class="controls input-xlarge text-tip">
                        @Html.TextBoxFor(m => m.UploadDate, new { Value = "" + DateTime.Now.ToString("d-MMM-yyyy") + "", @readonly = "readonly", style = "font-weight:bold;  margin-top: 5px; width:210px;" })
                        <br />
                        @Html.ValidationMessageFor(model => model.UploadDate)
                    </div>
                </div>


                <div id="divAttachedDocument" class="control-group control-label">

                    @Html.LabelFor(model => model.UploadExcelID, new { resize = "both;", style = "font-Weight:Bold; " })

                    <div class="controls input-xlarge text-tip">
                        @Html.FileFor(model => model.MyFile)
                        <br />
                        @Html.ValidationMessageFor(model => model.UploadExcelID)
                    </div>
                </div>

                <div class="form-actions" style="border: 1px solid #e5e5e5; text-align: right">
                    <input type="button" id="CreateCRMCallEnquiryDetailsRecord" value="@Resources.ButtonAction_Submit" class="btn btn-primary" />

                </div>

            </div>
        </div>
    </div>
</form>
<script type="text/javascript">
    $(document).ready(function () {
        CRMCallEnquiryDetails.Initialize();
    });
</script>


