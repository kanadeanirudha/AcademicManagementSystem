@model AMS.ViewModel.OrganisationCentrewiseDepartmentViewModel

<script src="@Url.Content("~/Scripts/jquery.validate.min.js") " type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/fnReloadAjax.js")" type="text/javascript"></script>

<div id="custom-content">
    <div class="block-header">
        <h2>&nbsp;<i class="zmdi zmdi-view-toc"></i>&nbsp;@Resources.FormCreate_CentrewiseDepartment</h2>
    </div>
    <div class="card">
        <div class="card-body card-padding">
            <div id="displayErrorMessage" role="alert" hidden class="alert">
                <button class="close" type="button"><span aria-hidden="true">×</span></button>
                <p id="messageContent"></p>
            </div>
            <form id="FormCreateOrganisationCentrewiseDepartment">

                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true)
                @Html.HiddenFor(model => model.DepartmentID)
                @Html.HiddenFor(model => model.CentreCode)
                @Html.HiddenFor(model => model.SelectedCentreName)


                <div class="col-sm-12">
                    <div class="form-group  fg-line">
                            <b>@Resources.DisplayMessage_AreYouSureYouWanttoAllocate <u>@Html.DisplayFor(model => model.DepartmentName)</u>  @Resources.DisplayMessage_to  <u>@Html.DisplayFor(model => model.SelectedCentreName)</u>  @Resources.Display_CentreQue</b>
                    </div>
                </div>



                <div class="form-group" style="margin-left: 2%; ">
                    <input class="btn btn-primary btn-sm m-t-10 waves-effect" value="@Resources.ButtonAction_Submit" id="CreateOrganisationCentrewiseDepartmentRecord" type="button" />
                    <input name="" id="closeBtn" type="button" class="btn btn-default" value="No" />
                </div>
            </form>

        </div>
    </div>
</div>
<script src="@Url.Content("~/Scripts/jquery.validate-1.15.0.js") " type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        
        OrganisationCentrewiseDepartment.Initialize();
    });
</script>


